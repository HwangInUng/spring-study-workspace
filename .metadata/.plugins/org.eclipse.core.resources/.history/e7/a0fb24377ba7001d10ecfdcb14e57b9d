package com.mvc.model;

public class MovieAdvisor {
	public String getAdvice(String movie) {
		String value = null;
		
		switch (movie) {
		case "인터":
			value = "우주 영화 중 최고";
			break;
		case "다크":
			value = "조커가 연기 잘함";
			break;
		case "마션":
			value = "식물 키우다 끝남";
			break;
		case "아이":
			value = "보고싶다 아이언맨";
			break;
		}
		return value;
	}
}
